  "Create New Command":
    prefix: "ricom"
    body: '''
      /*----------------------------------------------------------------------
      --| COMMAND ${1:handlerName}
      --|
      --| Author: ${2:$TM_FULLNAME}
      --| Version:  1.0
      --| Created: ${3:`date "+%d-%m-%Y"`}
      --| Last Mod: ${3}
      --| Requires: ${4:--}
      --|
      --| Summary: ${5:description}
      --|
      --| Format:  ${1} ${6:params}
      --|
      --| Parameters: ${7:--}
      --|
      --| Return: ${8:empty}
      ----------------------------------------------------------------------*/

      command ${1} ${9:params}
      	${10:# code...}
      end ${1}
      ${0}
    '''
  "Create New Controller":
    prefix: "ricont"
    body: '''
      <?rev

      global gControllerHandlers, gData

      put "${1:controllerName},index" into gControllerHandlers


      command ${1}
      	${0:# code...}
      end ${1}


      command index
      	# SET PAGE TITLE
      	put "${2:YourPageTitle}" into gData["pageTitle"]

      	# LOAD THE VIEW FILE
      	get rigLoadView("${1:controllerName}View")
      end index




      --| END OF ${1}.lc
      --| Location: ./system/application/controllers/${1}.lc
      ----------------------------------------------------------------------
    '''
  "Create New Function":
    prefix: "rifunc"
    body: '''
      /*----------------------------------------------------------------------
      --| FUNCTION ${1:functionName}
