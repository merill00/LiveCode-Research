      	${10:# code...}
      end ${1}
      ${0}
    '''
  "Create New Model":
    prefix: "rimodel"
    body: '''
      <?rev
      put gBASEPATH into gBASEPATH

      if gBASEPATH is "gBASEPATH" then
      	put "No direct script access allowed."
      	exit to top
      end if


      ${0}



      --| END OF ${1:modelName}.lc
      --| Location:  ./system/application/models/${1}.lc
      ----------------------------------------------------------------------
    '''
